    type Submodel
     ports
      signal in input;
      signal out output;
    end;
    implementation eq
parameters
	real beta = 0.001;
	real roundUp=100;
variables
	real H;
	real dist;
	real outSum;
	real inSum;
	real inD;
initialequations
	H=10;
	outSum=0;
	inSum=0;
	dist=0;
code
	outSum=(outSum+H*beta) mod roundUp;
	output = outSum;
if (input+0.1*roundUp)<inSum then
	inD=roundUp+input-inSum;
else
	inD=input-inSum;
end;
	if input==0 then
		inD=0;
	end;
	inSum=(inSum+inD) mod roundUp;
	
	H=H+inD-H*beta+dist;implementation_end;
  end;
